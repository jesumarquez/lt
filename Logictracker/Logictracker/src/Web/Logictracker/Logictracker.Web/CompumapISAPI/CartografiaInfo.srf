<?xml version="1.0" encoding="ISO-8859-1"?>
{{handler CmapMEP.dll/CartografiaInfo}}
<info>
    {{if HasLong}}
    <metrosxpixel>{{PixelXMetro}}</metrosxpixel>
    {{else}}
    <bbox 
        minlat="{{MapMinLat}}" 
        minlon="{{MapMinLon}}" 
        maxlat="{{MapMaxLat}}" 
        maxlon="{{MapMaxLon}}" />
    {{endif}}
</info>
