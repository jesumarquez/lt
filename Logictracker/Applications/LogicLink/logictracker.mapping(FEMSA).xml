<?xml version="1.0" encoding="utf-8" ?>

<Configuration xmlns="urn:Logictracker.Process.Import.Client">
    <Import>
        <DataSource Type="Csv">
          <Parameter Name="separator" Value=";" />
          <Parameter Name="folder" Value="C:\Program Files (x86)\Logictracker\LogicLink\Archivos\" />
          <Parameter Name="filename" Value="*.txt" />
          <Parameter Name="hasheaderrow" Value="false" />
        </DataSource>
               
        <Entity Type="DistribucionF">
            <Property Name="Centro" ColumnIndex="0"></Property>
            <Property Name="Ruta" ColumnIndex="1"></Property>
            <Property Name="Entrega" ColumnIndex="2"></Property>
            <Property Name="CodigoCliente" ColumnIndex="3"></Property>
            <Property Name="DescripcionCliente" ColumnIndex="4"></Property>
            <Property Name="Latitud" ColumnIndex="5"></Property>
            <Property Name="Longitud" ColumnIndex="6"></Property>
            <Property Name="Fecha" ColumnIndex="7"></Property>
            <Property Name="Hora" ColumnIndex="8"></Property>
            <Property Name="Cajas" ColumnIndex="9"></Property>
            <Property Name="Patente" ColumnIndex="10"></Property>
            <Property Name="Legajo" ColumnIndex="11"></Property>
        </Entity>
        
    </Import>
</Configuration>